<?xml version="1.0" encoding="iso-8859-1"?>
<root>
	<!-- Remove DC related controls, resize comboboxes to fill the rest of the space -->
	<windowclass name="rr_console" merge="join">
		<sheetdata>
			<rollModule name="check" merge="join">
				<showDC merge="delete"/>
				<comboWidth>140</comboWidth>
			</rollModule>
			<rollModule name="save" merge="join">
				<showDC merge="delete"/>
				<comboWidth>140</comboWidth>
			</rollModule>
			<rollModule name="skill" merge="join">
				<showDC merge="delete"/>
				<comboWidth>240</comboWidth>
			</rollModule>
		</sheetdata>
	</windowclass>
	
	<windowclass name="expanding_button_selector" merge="join">
		<sheetdata>
			<label name="skilldclabel">
				<invisible/>
			</label>
			<basicnumber name="dc">
				<invisible/>
			</basicnumber>
		</sheetdata>
	</windowclass>
</root>